{
  "extends": ["airbnb", "next/core-web-vitals", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "arrow-body-style": ["warn", "as-needed"]
  }
}
